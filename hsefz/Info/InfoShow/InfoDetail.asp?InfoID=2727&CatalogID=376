	function Remark()
	{
		if(AllTrim(document.form1.CommentContent.value) == "")
		{
			window.alert("评论内容不能为空!");
			return;
		}
		document.form1.action = "InfoRemark.asp?IsRemark=1&InfoID=2727&CatalogID=376";
		document.form1.submit();	
	}
	
	function SetImage(objImage)
	{
		if((objImage.width-(800-150))>0) objImage.width=550;
	}
	
