                    <script language="javascript" src="http://www.hsdefz.com/hsefz/library/pub.js"></script>
<script language="javascript">
	function Search()
	{
		var sKeywords = AllTrim(document.formInfoSearch.SearchKeyword.value); 
		
		if(sKeywords == "")
		{
			window.alert("请输入你要搜索的信息!");
			return;
		}
		document.formInfoSearch.action = "/hsefz/Info/InfoShow/InfoSearchSession.asp?SearchKeyword="+sKeywords;
		document.formInfoSearch.submit();
	}
	
	function KeyPress()
	{
		if(window.event.keyCode == 13)
		{
			Search();
			window.event.returnValue = false;
			window.event.cancelBubble = true;
		}	
	}
</script>
<style type="text/css">
